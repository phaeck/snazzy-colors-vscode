	"tokenColors": [
		{
			"name": "Strong",
			"scope": "strong, markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Emphasis",
			"scope": "emphasis, markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Comment",
			"scope": "comment, punctuation.definition.comment, string.quoted.docstring",
			"settings": {
				"foreground": "#616263",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Storage",
			"scope": "storage.type",
			"settings": {
				"foreground": "#f3f99d",
			}
		},
		{
			"name": "Type",
			"scope": "storage.type.primitive, support.type, entity.name.type, keyword.type, keyword.other.unit",
			"settings": {
				"foreground": "#9aedfe"
			}
		},
		{
			"name": "String",
			"scope": "string, meta.structure.dictionary.json string.quoted.double.json",
			"settings": {
				"foreground": "#5af78e"
			}
		},
		{
			"name": "Boolean",
			"scope": "constant.language.boolean",
			"settings": {
				"foreground": "#5af78e"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric, support.constant",
			"settings": {
				"foreground": "#5af78e"
			}
		},
		{
			"name": "Null",
			"scope": "constant.language.null",
			"settings": {
				"foreground": "#5af78e"
			}
		},
		{
			"name": "Undefined",
			"scope": "constant.language.undefined",
			"settings": {
				"foreground": "#5af78e"
			}
		},
		{
			"name": "Function definition",
			"scope": "meta.function entity.name.function",
			"settings": {
				"foreground": "#57c7ff"
			}
		},
		{
			"name": "Function call",
			"scope": "entity.name.function, meta.function-call, support.function, keyword.other.special-method, meta.require, variable.language.super, keyword.other.get, keyword.other.set",
			"settings": {
				"foreground": "#57c7ff"
			}
		},
		{
			"name": "Identifier",
			"scope": "variable, support.variable, support.class, support.constant, meta.definition.variable entity.name.function",
			"settings": {
				"foreground": "#eff0eb"
			}
		},
		{
			"name": "Constants",
			"scope": "variable.other.constant.property",
			"settings": {
				"foreground": "#f3f99d"
			}
		},
		{
			"name": "Class",
			"scope": "entity.name.type.class",
			"settings": {
				"foreground": "#ff5c57"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class entity.name.type.module",
			"settings": {
				"foreground": "#ff5c57",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Class variable",
			"scope": "variable.object.property",
			"settings": {
				"foreground": "#f3f99d"
			}
		},
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Class method",
			"scope": "meta.definition.method entity.name.function",
			"settings": {
				"foreground": "#f3f99d"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword, modifier, storage.modifier, support.type.object, constant.language, variable.language.this, variable.language.special.self",
			"settings": {
				"foreground": "#ff6ac1"
			}
		},
		{
			"name": "Storage Modifier [e.g. class, extends, static] (JS)",
			"scope": "storage.modifier, storage.type.class.js, variable.language.this, variable.language.this.js",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Modules",
			"scope": "support.module, support.node",
			"settings": {
				"foreground": "#ff5c57",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Template expression",
			"scope": "template.expression.begin, template.expression.end",
			"settings": {
				"foreground": "#ff6ac1"
			}
		},
		{
			"name": "Variable sigils",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.definition.variable"
			],
			"settings": {
				"foreground": "#ff5c57"
			}
		},
		{
			"name": "Punctuation accessor with functions",
			"scope": "meta.function-call.ts punctuation.accessor",
			"settings": {
				"foreground": "#57c7ff"
			}
		},
		{
			"name": "Punctuation accessor others",
			"scope": "punctuation.accessor",
			"settings": {
				"foreground": "#ff6ac1"
			}
		},
		{
			"name": "Exception",
			"scope": "support.type.exception",
			"settings": {
				"foreground": "#f3f99d"
			}
		},
		{
			"name": "Diff insertion",
			"scope": "markup.inserted.diff, meta.diff.header.to-file",
			"settings": {
				"foreground": "#5af78e"
			}
		},
		{
			"name": "Diff deletion",
			"scope": "markup.deleted.diff, meta.diff.header.from-file",
			"settings": {
				"foreground": "#ff5c57"
			}
		},
		{
			"name": "TypeScript decorator",
			"scope": "punctuation.decorator.ts",
			"settings": {
				"foreground": "#9aedfe"
			}
		},
		{
			"name": "TypeScript type parameters brackets",
			"scope": [
				"punctuation.definition.typeparameters.begin.ts",
				"punctuation.definition.typeparameters.end.ts"
			],
			"settings": {
				"foreground": "#ff6ac1"
			}
		},
		{
			"name": "Getter, Setter",
			"scope": "storage.type.property",
			"settings": {
				"foreground": "#ff6ac1"
			}
		},
		{
			"name": "C# class",
			"scope": "source.cs meta.class.identifier storage.type",
			"settings": {
				"foreground": "#ff5c57"
			}
		},
		{
			"name": "C# class method",
			"scope": "source.cs meta.method.identifier entity.name.function",
			"settings": {
				"foreground": "#f3f99d"
			}
		},
		{
			"name": "C# method call",
			"scope": "source.cs meta.method-call meta.method, source.cs entity.name.function",
			"settings": {
				"foreground": "#57c7ff"
			}
		},
		{
			"name": "C# type",
			"scope": "source.cs storage.type",
			"settings": {
				"foreground": "#f3f99d"
			}
		},
		{
			"name": "C# return type",
			"scope": "source.cs meta.method.return-type",
			"settings": {
				"foreground": "#9aedfe"
			}
		},
		{
			"name": "C# preprocessor",
			"scope": "source.cs meta.preprocessor",
			"settings": {
				"foreground": "#616263"
			}
		},
		{
			"name": "C# namespace",
			"scope": "source.cs entity.name.type.namespace",
			"settings": {
				"foreground": "#eff0eb"
			}
		},
		{
			"name": "CSS class",
			"scope": "entity.other.attribute-name.class",
			"settings": {
				"foreground": "#9aedfe"
			}
		},
		{
			"name": "CSS id",
			"scope": "entity.other.attribute-name.id",
			"settings": {
				"foreground": "#9aedfe"
			}
		},
		{
			"name": "CSS tag",
			"scope": "source.css entity.name.tag",
			"settings": {
				"foreground": "#f3f99d"
			}
		},
		{
			"name": "CSS property",
			"scope": "source.css support.type.property-name",
			"settings": {
				"foreground": "#ff5c57"
			}
		},
		{
			"name": "HTML tag outer",
			"scope": "meta.tag, punctuation.definition.tag",
			"settings": {
				"foreground": "#eff0eb"
			}
		},
		{
			"name": "HTML tag inner",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#ff5c57"
			}
		},
		{
			"name": "HTML tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#f3f99d",
				"fontStyle": "italic"
			}
		},
		{
			"name": "HTML tag expression",
			"scope": "punctuation.section.embedded",
			"settings": {
				"foreground": "#ff5c57"
			}
		},
		{
			"name": "INI property name",
			"scope": "keyword.other.definition.ini",
			"settings": {
				"foreground": "#ff5c57"
			}
		},
